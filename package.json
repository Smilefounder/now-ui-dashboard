{
  "name": "now-ui-dashboard-pro",
  "description": "Premium Bootstrap 4 Design",
  "version": "1.0.0",
  "keywords": [
    "material",
    "design",
    "bootstrap",
    "css",
    "js",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "http://creative-tim.com/product/now-ui-dashboard-pro",
  "author": "Creative Tim",
  "scripts": {
    "docs:compile": "bundle exec jekyll build",
    "product:compile": "JEKYLL_ENV=production bundle exec jekyll build",
    "docs:serve": "bundle exec jekyll serve",
    "docs:assets": "node-sass --importer=node_modules/node-sass-tilde-importer assets/scss/docs.scss > assets/css/docs.css && cssnano assets/css/docs.css assets/css/docs.min.css",
    "docs:deploy": "npm run docs:assets && npm run docs:compile && ./deploy.sh",
    "product:deploy": "npm run docs:assets && npm run product:compile && ./deploy.sh",
    "prepare": "npm run build",
    "build": "npm run build:js-min && npm run build:css && npm run build:css-min",
    "watch": "concurrently --kill-others --names \"js,css\" \"npm run watch:js\" \"npm run watch:css\"",
    "watch:js": "npm run build:js -- --watch",
    "watch:css": "npm run build:css && npm run build:css -- --watch",
    "build:js-min": "minify assets/js/now-ui-dashboard.js --out-file assets/js/now-ui-dashboard.min.js",
    "build:css": "node-sass --importer=node_modules/node-sass-tilde-importer --output-style expanded --source-map true --source-map-contents true --precision 6 assets/scss/now-ui-dashboard/now-ui-dashboard.scss assets/css/now-ui-dashboard.css",
    "build:css-min": "cssnano assets/css/now-ui-dashboard.css assets/css/now-ui-dashboard.min.css",
    "postpublish": "GIT_DEPLOY_DIR=dist GIT_DEPLOY_BRANCH=dist ./deploy.sh",
    "css-lint": "bundle exec scss-lint --config .sass-lint.yml assets/_scss/**/*.scss"
  },
  "style": "assets/css/material-kit.css",
  "sass": "scss/_core.scss",
  "main": "assets/js/material-kit.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/creativetimofficial/now-ui-dashboard-pro.git"
  },
  "bugs": {
    "url": "https://github.com/creativetimofficial/now-ui-dashboard-pro/issues"
  },
  "license": "Creative Tim License <www.creative-tim.com/license>",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-minify": "^0.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-es2015-modules-strip": "^0.1.1",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "4.0.0-beta",
    "clean-css-cli": "^4.1.6",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "cssnano-cli": "^1.0.5",
    "eslint": "^4.2.0",
    "grunt": "^0.4.5",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-jsbeautifier": "^0.2.13",
    "grunt-shell": "^1.1.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-deporder": "^1.1.0",
    "gulp-html-prettify": "0.0.1",
    "gulp-imagemin": "^3.3.0",
    "gulp-install": "^1.1.0",
    "gulp-jsbeautifier": "^2.1.1",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^7.0.0",
    "gulp-remove-lines": "^0.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-html-validator": "0.0.5",
    "gulp-stylelint": "^4.0.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^4.0.0",
    "jquery": ">=3.0.0",
    "load-grunt-tasks": "^0.6.0",
    "node-sass": "^4.5.3",
    "node-sass-tilde-importer": "^1.0.0",
    "popper.js": "^1.11.0",
    "postcss-assets": "^4.1.0",
    "prettier": "^1.5.3",
    "time-grunt": "^0.4.0"
  },
  "files": [
    "assets",
    "js/*.js",
    "scss/**/*.scss",
    "LICENSE"
  ],
  "dependencies": {
    "gulp-git": "^2.4.2",
    "gulp-html-validator": "0.0.5",
    "gulp-install": "^1.1.0",
    "gulp-remove-lines": "^0.1.0",
    "gulp-replace": "^0.6.1"
  }
}
